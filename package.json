{
  "name": "playwright-typescript-template",
  "version": "1.0.0",
  "description": "Project automation framework template by Playwright Typescript",
  "main": "index.js",
  "scripts": {
    "tests:e2e": "npx playwright test --project=TestOnChrome",
    "tests:e2e-basic": "npx playwright test --grep @smoke --project=TestOnChrome",
    "tests:e2e-firefox": "npx playwright test --project=TestOnFireFox",
    "tests:e2e-edge": "npx playwright test --project=TestOnEdge",
    "tests:e2e-allbrowsers": "npx playwright test --project=MultiBrowser",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:report": "eslint . --ext .ts --format html --output-file eslint-report.html"
  },
  "keywords": [],
  "author": "Wizeline",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "20.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "exceljs": "^4.4.0",
    "ts-node": "^10.9.2"
  }
}
